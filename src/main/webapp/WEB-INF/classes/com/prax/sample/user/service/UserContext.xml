<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!-- User Service Bean -->
	<bean id="SampleUserHibDao" parent="baseHibDao">
		<constructor-arg><value>com.prax.sample.user.entity.User</value></constructor-arg>
	</bean>
	<bean id="SampleUserService" class="com.prax.sample.user.service.UserServiceImpl"  parent="baseService">
		<property name="dao" ref="SampleUserHibDao"/>
	</bean>
</beans>