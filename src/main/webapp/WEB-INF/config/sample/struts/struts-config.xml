<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>

	<action-mappings type="com.prax.framework.base.action.BaseActionMapping">
		<action path="/sample/user/default" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<forward name="SUCCESS" path=".sample.user.default"/>
		</action>
		<action path="/sample/user/list" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<set-property property="dataAttribute" value="page"/>
			<forward name="SUCCESS" path=".sample.user.list"/>
		</action>
		<action path="/sample/user/add" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<set-property property="dataAttribute" value="user"/>
			<forward name="SUCCESS" path=".sample.user.add"/>
		</action>
		<action path="/sample/user/edit" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<set-property property="dataAttribute" value="user"/>
			<forward name="SUCCESS" path=".sample.user.edit"/>
		</action>
		<action path="/sample/user/save" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<set-property property="dataAttribute" value="user"/>
			<forward name="SUCCESS" path="/sample/user/list.do"/>
		</action>
		<action path="/sample/user/delete" type="com.prax.sample.user.web.UserAction" name="bindingForm" scope="request">
			<set-property property="dataAttribute" value="user"/>
			<forward name="SUCCESS" path="/sample/user/list.do"/>
		</action>
	</action-mappings>
	
</struts-config>
